{
    "name": "cmsmean",
    "version": "1.1.0",
    "description": "",
    "main": "dist/index.js",
    "scripts": {
        "dev": "tsc-watch --onSuccess \"node -r ./tsconfig-paths-bootstrap.js --stack_trace_limit=100 -r trace -r clarify dist/index.js\"",
        "build": "npm run clean:dist && tsc -p tsconfig.prod.json",
        "start": "npm run clean:dist && tsc -p tsconfig.prod.json && cross-env NODE_ENV=production node -r ./tsconfig-paths-bootstrap.js dist/index.js",
        "serve": "cross-env NODE_ENV=production node -r ./tsconfig-paths-bootstrap.js dist/index.js",
        "start:pm2": "pm2 start pm2.json",
        "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit",
        "coverage": "cross-env NODE_ENV=test jest --coverage",
        "majestic": "npx majestic",
        "docs": "npm run clean:docs && jsdoc -c ./jsdoc.json ",
        "clean:dist": "rimraf dist/*",
        "clean:docs": "rimraf docs/*"
    },
    "keywords": [],
    "author": "Reiva",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.0.1",
        "clarify": "^2.1.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "express": "^4.17.1",
        "express-status-monitor": "^1.3.3",
        "fastest-validator": "^1.11.1",
        "figlet": "^1.5.2",
        "helmet": "^5.0.0",
        "http-status-codes": "^2.1.4",
        "ioredis": "^4.27.9",
        "jsonwebtoken": "^8.5.1",
        "memory-cache": "^0.2.0",
        "mongodb": "^4.1.2",
        "mongoose": "^6.0.2",
        "mongoose-paginate-v2": "^1.4.2",
        "morgan": "^1.10.0",
        "rate-limiter-flexible": "^2.3.0",
        "swagger-stats": "^0.99.2",
        "swagger-ui-express": "^4.1.6",
        "trace": "^3.1.1",
        "tsconfig-paths": "^3.11.0",
        "validator": "^13.7.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/compression": "1.7.2",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/figlet": "^1.5.4",
        "@types/helmet": "^4.0.0",
        "@types/ioredis": "4.28.8",
        "@types/jest": "27.4.1",
        "@types/jsonwebtoken": "8.5.8",
        "@types/memory-cache": "0.2.2",
        "@types/mongoose-paginate-v2": "1.4.3",
        "@types/morgan": "^1.9.3",
        "@types/node": "16.11.26",
        "@types/redis": "4.0.10",
        "@types/supertest": "^2.0.11",
        "@types/swagger-stats": "^0.95.4",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/validator": "13.7.1",
        "better-docs": "2.7.2",
        "husky": "7.0.4",
        "jest": "27.5.1",
        "jsdoc": "3.6.10",
        "mongodb-memory-server": "8.4.0",
        "rimraf": "^3.0.2",
        "supertest": "6.2.2",
        "ts-jest": "27.1.3",
        "tsc-watch": "4.6.1",
        "tsconfig-paths-jest": "0.0.1",
        "typescript": "4.6.2"
    },
    "majestic": {
        "args": [
            "--runInBand --detectOpenHandles --config=./jest.config.js"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run build",
            "pre-push": "npm run build"
        }
    }
}
